{
    "name": "MT-TTS语音合成",
    "id": "MT-TTS",
    "version": "1.0.3",
    "description": "安卓tts语音合成",
    "_dp_type":"nativeplugin",
    "_dp_nativeplugin":{
        "android": {
            "plugins": [
                {
                "type": "module",
                "name": "MT-TTS-Baidu",
